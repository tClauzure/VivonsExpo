<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vivonsexpos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VivonsExpos">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuStaff"></activity>
        <activity android:name=".MenuExposant"></activity>
        <activity android:name=".MenuExposantMesStands"></activity>
        <activity android:name=".MenuExposantModif"></activity>
        <activity android:name=".MenuInscription"></activity>
        <activity android:name=".MenuConnexion"></activity>
    </application>

</manifest>